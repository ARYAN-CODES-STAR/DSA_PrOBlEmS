contest problem:--

class Solution {
public:
    
    
    int countDigits(int num) {
        int cnt = 0;
        
        while( num>0 ){
            int temp = num%10 ;
           if( num%temp == 0){
               cnt++ ;
               num=num/10 ;
           }
                             
            }
        return cnt ;
            
        }  
    
};
