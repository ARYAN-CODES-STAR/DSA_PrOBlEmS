// print sum of all elements in an array

#include<iostream>
using namespace std;
int sums( int arr[] , int n ){
    
    int sum = 0 ;
    for( int i = 0 ; i < n ; i++ ){
        sum = sum + arr[i] ;
    }
    return sum ;
}
int main(){
    
    
    int a[5] ={1,2,3,4,5};
    
     sums( a,5 );
    
    cout<< sums(a,5);
    
    
    return 0;
}
